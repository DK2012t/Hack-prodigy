function flipTime() {
PIXI.game.prodigy.user.width=PIXI.game.prodigy.user.width*-1;
PIXI.game.prodigy.user.nameText.gameObject.scale=-1;
}
function setLocation() {
  PIXI.game.prodigy.player.locationSelectionType = prompt("What will your location string be?", "your kitchen making a sandwich");
}
function setNickname() {
  PIXI.game.state.states.Boot._gameData.nickname[PIXI.game.prodigy.player.name.nickname] = prompt("Set your clientside nickname (nobody else can see it but you) here.", "{first} {last}, but you can call me Einstein");
}
function fakeFriend() {
PIXI.game.prodigy.debugMisc.addFakeFriend()
}
function getMonster() {
PIXI.game.prodigy.player.kennel.addPet=152
}
function getallItem() {
PIXI.game.prodigy.player.backpack.data.item=[]
x = PIXI.game.state.states.Boot._gameData.item
for (i in x) {
    PIXI.game.prodigy.player.backpack.data.item[i] = {"ID": x[i].ID, "N": Infinity}
}
}
function getMaxDamage() {
PIXI.game.prodigy.player.modifiers.damage = 99999999999
}
function battleTime() {
PIXI.game.prodigy.debugMisc.startBattle()
}
function fontSize() {
PIXI.game.prodigy.user.chatText.FontSize = 100
}
function getMaxHearts() {
PIXI.game.prodigy.player.modifiers.maxHearts = 99999999999
}
function escape() {
PIXI.game.prodigy.debugMisc.escapeBattle()
}
function getAllKeystones() {
PIXI.game.prodigy.debugMisc.getKeyItems()
}
function breakGame() {
PIXI.game.prodigy.debugMisc.setFPS(1)
}
function fixLag() {
PIXI.game.prodigy.debugMisc.setFPS(60)
}
function getMaxWins() {
PIXI.game.prodigy.player.data.win = 999999999
}
